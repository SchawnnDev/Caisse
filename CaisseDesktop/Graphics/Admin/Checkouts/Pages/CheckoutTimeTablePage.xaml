<local:CustomPage x:Class="CaisseDesktop.Graphics.Admin.Checkouts.Pages.CheckoutTimeTablePage"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:local="clr-namespace:CaisseDesktop.Graphics.Admin"
                  mc:Ignorable="d"
                  d:DesignHeight="450" d:DesignWidth="800"
                  Title="CheckoutTimeTablePage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition MaxWidth="50" />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition MaxWidth="50"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition MaxHeight="50"/>
            <RowDefinition/>
            <RowDefinition MaxHeight="50"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Vertical">

            <Viewbox Stretch="Fill">
                <Rectangle Fill="CornflowerBlue" />
            </Viewbox>
            <Rectangle Height="Auto" Fill="IndianRed" />

        </StackPanel>

        <StackPanel Grid.Row="1" Grid.Column="2" Orientation="Vertical">

            <Rectangle Height="Auto" Fill="CornflowerBlue" />
            <Rectangle Height="Auto" Fill="IndianRed" />

        </StackPanel>

    </Grid>
</local:CustomPage>